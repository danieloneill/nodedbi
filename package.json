  },
  "engines": {
    "node": ">=0.11.13"
  },
  "gypfile": true,
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/danieloneill/nodedbi/issues"
  },
  "homepage": "https://github.com/danieloneill/nodedbi"
}
