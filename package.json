{
  "name": "nodedbi",
  "version": "1.0.8",
  "description": "A LibDBI interface for Node.js",
  "author": {
    "name": "Daniel O'Neill",
    "email": "doneill@akielectronics.com",
    "url": "http://akielectronics.com"
  },
  "keywords": [
    "database",
    "dbi",
    "libdbi",
    "mysql",
    "postgresql",
    "sql"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/danieloneill/nodedbi.git"
